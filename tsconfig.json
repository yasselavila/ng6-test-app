{
  "compilerOptions": {
    "target": "es5",
    "moduleResolution": "node",
    "outDir": "./dist/out-tsc",
    "baseUrl": "./",
    "declaration": false,
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "noImplicitAny": true,
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ],
    "paths": {
      "@app/env": [
        "src/environments/environment.ts"
      ],
      "@app/*": [
        "src/*"
      ]
    }
  },
  "angularCompilerOptions": {
    "preserveWhiteSpace": false,
    "fullTemplateTypeCheck": true
  },
  "compileOnSave": false,
  "buildOnSave": false,
  "atom": {
    "rewriteTsconfig": false
  }
}
